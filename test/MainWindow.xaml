<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" x:Class="test.MainWindow"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <dxg:GridControl ItemsSource="{Binding Data.results}">
            <dxg:GridControl.View>
                <dxg:TableView ShowTotalSummary="False"/>
            </dxg:GridControl.View>
            <dxg:GridColumn FieldName="s.id"/>
            <dxg:GridColumn FieldName="s.name"/>
            <dxg:GridColumn FieldName="s.age"/>
            <dxg:GridColumn FieldName="s.tags"/>
            <dxg:GridColumn FieldName="s.s.name"/>
        </dxg:GridControl>

    </Grid>
</Window>
